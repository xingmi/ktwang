<div class="container project_content">
  <div class="row">
    <div class="col-md-9">
      <ol class="breadcrumb">
        <li><a href="/">首页</a></li>
        {{#foreach category.breadcrumb}}
          <li><a href="{{path}}">{{name}}</a></li>
        {{/foreach}}
        <li><a href="{{category.path}}">{{category.name}}</a></li>
        <li class="active">{{text title words="15"}}</li>
      </ol>
      <div class="page-header text-center">
        <h1 class="project_title">{{title}}</h1>
        <p class="text-muted">{{dateformat date format="YYYY年MM月DD日 HH:mm"}}　参与人数：{{reading.total}}　编辑：<a href="mailto:{{user.email}}" title="邮件联系{{user.nickname}}">{{user.nickname}}</a>        <a target="_blank" href="//shang.qq.com/wpa/qunwpa?idkey=73a31729cf977e9b310639c7338096aa3342e7ff58ecc0768a7a1576d276ada1"><img border="0" src="//pub.idqqimg.com/wpa/images/group.png" alt="空投撸币" title="空投撸币"></a></p>

      </div>
      {{#if abstract}}
        <div class="well text-muted">{{abstract}}</div>
      {{/if}}
      <div class="content">
        {{{content}}}
      </div>
      {{#if tags}}
        <hr>
        <div>
          标签：{{#foreach tags}}<span>{{this}}</span> {{/foreach}}
        </div>
      {{/if}}
    </div>
    <div class="col-md-3 hidden-xs hidden-sm">
      <div class="panel panel-default">
        <div class="panel-heading">栏目阅读总排行</div>
        <ul class="list-group">
          {{#foreach readingList.total limit="5"}}
            <a href="{{href}}" class="list-group-item text-overflow">{{title}}</a>
          {{/foreach}}
        </ul>
      </div>
      <div class="panel panel-default">
        <div class="panel-heading">栏目阅读本月排行</div>
        <ul class="list-group">
          {{#foreach readingList.month limit="5"}}
            <a href="{{href}}" class="list-group-item text-overflow">{{title}}</a>
          {{/foreach}}
        </ul>
      </div>
      <div class="panel panel-default">
        <div class="panel-heading">栏目阅读本周排行</div>
        <ul class="list-group">
          {{#foreach readingList.week limit="5"}}
            <a href="{{href}}" class="list-group-item text-overflow">{{title}}</a>
          {{/foreach}}
        </ul>
      </div>
      <div class="panel panel-default">
        <div class="panel-heading">栏目阅读今日排行</div>
        <ul class="list-group">
          {{#foreach readingList.day limit="5"}}
            <a href="{{href}}" class="list-group-item text-overflow">{{title}}</a>
          {{/foreach}}
        </ul>
      </div>
    </div>
  </div>
</div>